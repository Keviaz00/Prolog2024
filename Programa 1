using System;
using System.Collections.Generic;

public class Naturales
{
    // Genera una lista de números naturales desde 0 hasta N-1
    public static List<int> Natu(int n)
    {
        List<int> resultado = new List<int>();
        for (int i = 0; i < n; i++)
        {
            resultado.Add(i);
        }
        return resultado;
    }
    
    // Ejemplo de uso
    public static void Main()
    {
        int N = 5; // Cambia este valor para generar más números naturales
        List<int> numerosNaturales = Natu(N);
        Console.WriteLine(string.Join(", ", numerosNaturales)); // Imprime: 0, 1, 2, 3, 4
    }
}
