class Fibonacci
{
    public static int[] Generate(int n)
    {
        int[] fib = new int[n];
        fib[0] = 0;
        fib[1] = 1;

        for (int i = 2; i < n; i++)
        {
            fib[i] = fib[i - 1] + fib[i - 2]; // Cada nÃºmero es la suma de los dos anteriores
        }

        return fib; // Retorna la secuencia de Fibonacci
    }
}
